n=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    <div class=\"container-fluid\">\n        <div class=\"row mbr-white\">\n            <div class=\"footer-item col-12 col-md-6 col-lg-4\">\n                <div class=\"col-3 mb-4\" mbr-if=\"showLogo\">\n                    <div class=\"media-wrap col-{{4*logoSize}}\">\n                        <a href=\"https://dzi.asia\">\n                            <img src=\"@PROJECT_PATH@/assets/images/da-logo540-540x540.png\" alt=\"Dzi.Asia Beads Store\">\n                        </a>\n                    </div>\n                </div>\n                <div mbr-form class=\"col-12 mx-auto mbr-form mb-3\">\n                    <div class=\"mbr-section-head mb-3\" mbr-if=\"showTitle\">\n                        <h3 class=\"mbr-section-title mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-section-title\">\n                            <p class=\"mb-0\">Get new products update</p>\n                        </h3>\n                    </div>\n                    <form action name=\"Form Name\" method=\"POST\" class=\"mbr-form form-with-styler\">\n                        <div>\n                            <div hidden=\"hidden\" data-form-alert class=\"alert alert-success col-12\">Thanks for filling out\n                                the form!</div>\n                            <div hidden=\"hidden\" data-form-alert-danger class=\"alert alert-danger col-12\">Oops...! some\n                                problem!</div>\n                        </div>\n                        <div class=\"dragArea\">\n                            <div class=\"col-md col-8 form-group mb-0\" data-for=\"email\">\n                                <input type=\"email\" name=\"email\" placeholder=\"E-mail\" data-form-field=\"email\" class=\"form-control\" value>\n                