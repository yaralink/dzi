n=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    <div class=\"container-fluid\">\n        <div class=\"row mbr-white\">\n            <div class=\"footer-item col-12 col-md-6 col-lg-4\">\n                <div class=\"col-3 mb-4\" mbr-if=\"showLogo\">\n                    <div class=\"media-wrap col-{{4*logoSize}}\">\n                        <a href=\"https://dzi.asia\">\n                            <img src=\"@PROJECT_PATH@/assets/images/da-logo540-540x540.png\" alt=\"Dzi.Asia Beads Store\">\n                        </a>\n                    </div>\n                </div>\n                <div mbr-form class=\"col-12 mx-auto mbr-form mb-3\">\n                    <div class=\"mbr-section-head mb-3\" mbr-if=\"showTitle\">\n                        <h3 class=\"mbr-section-title mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-section-title\">\n                            <p class=\"mb-0\">Get new products update</p>\n                        </h3>\n                    </div>\n                    <form action name=\"Form Name\" method=\"POST\" class=\"mbr-form form-with-styler\">\n                        <div>\n                            <div hidden=\"hidden\" data-form-alert class=\"alert alert-success col-12\">Thanks for filling out\n                                the form!</div>\n                            <div hidden=\"hidden\" data-form-alert-danger class=\"alert alert-danger col-12\">Oops...! some\n                                problem!</div>\n                        </div>\n                        <div class=\"dragArea\">\n                            <div class=\"col-md col-8 form-group mb-0\" data-for=\"email\">\n                                <input type=\"email\" name=\"email\" placeholder=\"E-mail\" data-form-field=\"email\" class=\"form-control\" value>\n                            </div>\n                            <div mbr-buttons=\"true\" mbr-theme-style=\"display-4\" data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\" class=\"mbr-section-btn\">\n                                <a type=\"submit\" class=\"btn btn-info\">Send</a>\n                            </div>\n                        </div>\n                    </form>\n                </div>\n            </div>\n\n            <div class=\"footer-item footer-item_links col-12 col-md-6 col-lg-4\">\n                <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style mb-4 section-subtitle\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\">\n                    <b>Find</b>\n                </h5>\n                <div class=\"footer-list-container\">\n                    <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline mbr-article>\n                        <li class=\"mbr-text item-wrap\">\n                            <p class=\"link\" mbr-theme-style=\"display-7\" data-app-selector=\".link\" mbr-if=\"showLink\">\n                                <a class=\"text-info custom-link\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">About</a>\n                            </p>\n                        </li>\n                        <li class=\"mbr-text item-wrap\">\n                            <p class=\"link\" mbr-theme-style=\"display-7\" data-app-selector=\".link\" mbr-if=\"showLink\">\n                                <a class=\"text-info custom-link\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">Blog</a>\n                            </p>\n                        </li>\n                        <li class=\"mbr-text item-wrap\">\n                            <p class=\"link\" mbr-theme-style=\"display-7\" data-app-selector=\".link\" mbr-if=\"showLink\">\n                                <a class=\"text-info custom-link\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">Portfolio</a>\n                            </p>\n                        </li>\n                        <li class=\"mbr-text item-wrap\">\n                            <p class=\"link\" mbr-theme-style=\"display-7\" data-app-selector=\".link\" mbr-if=\"showLink\">\n                                <a class=\"text-info custom-link\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">GridM5<b>Story</b></a>\n                            </p>\n                        </li>\n                        <li class=\"mbr-text item-wrap\">\n                            <p class=\"link\" mbr-theme-style=\"display-7\" data-app-selector=\".link\" mbr-if=\"showLink\">\n                                <a class=\"text-info custom-link\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">Our<b>Team</b></a>\n                            </p>\n                        </li>\n                    </ul>\n                    <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list\" data-multiline mbr-article>\n                        <li class=\"mbr-text item-wrap\">\n                            <p class=\"link\" mbr-theme-style=\"display-7\" data-app-selector=\".link\" mbr-if=\"showLink\">\n                                <a class=\"text-info custom-link\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">Testimonials</a>\n                            </p>\n                        </li>\n                        <li class=\"mbr-text item-wrap\">\n                            <p class=\"link\" mbr-theme-style=\"display-7\" data-app-selector=\".link\" mbr-if=\"showLink\">\n                                <a class=\"text-info custom-link\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">Blog</a>\n                            </p>\n                        </li>\n                        <li class=\"mbr-text item-wrap\">\n                            <p class=\"link\" mbr-theme-style=\"display-7\" data-app-selector=\".link\" mbr-if=\"showLink\">\n                                <a class=\"text-info custom-link\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\"></a><a href=\"page7.html\" class=\"text-primary\">Open</a><b><a href=\"page7.html\" class=\"text-primary\">Roles</a></b>\n                            </p>\n                        </li>\n                        <li class=\"mbr-text item-wrap\">\n                            <p class=\"link\" mbr-theme-style=\"display-7\" data-app-selector=\".link\" mbr-if=\"showLink\">\n                                <a class=\"text-info custom-link\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">Book a <b>call</b></a>\n                            </p>\n                        </li>\n                        <li class=\"mbr-text item-wrap\">\n                            <p class=\"link\" mbr-theme-style=\"display-7\" data-app-selector=\".link\" mbr-if=\"showLink\">\n                                <a class=\"text-info custom-link\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">Our<b>Team</b></a>\n                            </p>\n                        </li>\n                    </ul>\n                </div>\n            </div>\n\n            <div class=\"footer-item footer-item_contacts col-12 col-md-6 col-lg-4\">\n                <h5 class=\"mbr-section-subtitle mbr-fonts-style mb-4 section-subtitle\" mbr-theme-style=\"display-4\" mbr-if=\"showTitle\"><b>Contacts</b></h5>\n                <p class=\"mbr-text mbr-fonts-style mb-1 small-label\" mbr-theme-style=\"display-7\" data-app-selector=\".small-label\" mbr-if=\"showAddress\">\n                    address</p>\n                <p class=\"mbr-text mbr-fonts-style mb-3 contacts-text\" mbr-theme-style=\"display-7\" data-app-selector=\".contacts-text\" mbr-if=\"showAddress\">\n                    Kathmandu, Nepal</p>\n                <p class=\"mbr-text mbr-fonts-style mb-1 small-label\" mbr-theme-style=\"display-7\" data-app-selector=\".small-label\" mbr-if=\"showPhone\">\n                    phone</p>\n                <p class=\"mbr-text mbr-fonts-style mb-3 contacts-text\" mbr-theme-style=\"display-7\" data-app-selector=\".contacts-text\" mbr-if=\"showPhone\">\n                    +977.981.3720575</p>\n                <p class=\"mbr-text mbr-fonts-style mb-1 small-label\" mbr-theme-style=\"display-7\" data-app-selector=\".small-label\" mbr-if=\"showEmail\">\n                    email</p>\n                <p class=\"mbr-text mbr-fonts-style mb-3 contacts-text\" mbr-theme-style=\"display-7\" data-app-selector=\".contacts-text\" mbr-if=\"showEmail\">\n                    dzi@dzi.asia</p>\n                <p class=\"mbr-text mbr-fonts-style mb-1 small-label\" mbr-theme-style=\"display-7\" data-app-selector=\".small-label\" mbr-if=\"showSocial\">social</p>\n                <div class=\"social-row display-5\" mbr-if=\"showSocial\">\n                    <div class=\"soc-item\">\n                        <a href=\"https://t.me/dzi.asia\" target=\"_blank\">\n                            <span mbr-icon class=\"mbr-iconfont socicon-telegram socicon\"></span>\n                        </a>\n                    </div>\n                    <div class=\"soc-item\" mbr-if=\"socialCount>1\">\n                        <a href=\"https://vk.com/dzi.asia\" target=\"_blank\">\n                            <span mbr-icon class=\"mbr-iconfont socicon-vkontakte socicon\"></span>\n                        </a>\n                    </div>\n                    <div class=\"soc-item\" mbr-if=\"socialCount>2\">\n                        <a href=\"https://blog.dzi.asia/\" target=\"_blank\">\n                            <span mbr-icon class=\"mbr-iconfont socicon-blogger socicon\"></span>\n                        </a>\n                    </div>\n                    <div class=\"soc-item\" mbr-if=\"socialCount>3\">\n                        <a href=\"https://twitter.com/dziasiabeads\" target=\"_blank\">\n                            <span mbr-icon class=\"mbr-iconfont socicon-twitter socicon\"></span>\n                        </a>\n                    </div>\n                </div>\n            </div>\n\n        </div>\n    </div>\n</section>",
